        <div class="container">
            <div class="row my-5 rounded border bg-white">
                <div class="col-md-4 mx-auto">
                    <div class="avatar-upload">
                        <div class="avatar-preview">
                            <div id="imagePreview" style="background-image: url({{result.Profile.Profile1.photo}});">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col ml-5">
                    <div class="row">
                        <h5 class="mt-5">
                            {{result.Profile.Profile1.name.firstname}}&nbsp;{{result.Profile.Profile1.name.middlename}}&nbsp;{{result.Profile.Profile1.name.lastname}}
                            <hr>
                        </h5>
                    </div>
                    <div class="row">
                        <label>About me:</label>
                        <p>{{result.Profile.Profile3.AboutYourself}}</p>
                    </div>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 order-lg-2">
                            <ul class="nav nav-tabs mb-2">
                                <li class="nav-item ">
                                    <a href="" data-target="#profile" data-toggle="tab"
                                        class="nav-link active">Profile</a>
                                </li>
                                <li class="nav-item">
                                    <a href="" data-target="#partnerPreference" data-toggle="tab" class="nav-link">
                                        Partner
                                        Preference</a>
                                </li>
                                <li class="nav-item">
                                    <a href="" data-target="#photoAlbum" data-toggle="tab" class="nav-link">
                                        Album</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal fade" id="confirm-update" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
                aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">

                        <div class="modal-header">
                            <h4>Confirm Update</h4>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            <h4 class="modal-title" id="myModalLabel"></h4>
                        </div>

                        <div class="modal-body">
                            <p>Are you sure you want to update?</p>
                        </div>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                            <a class="btn btn-info btn-ok">Update</a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-content py-4 bg-white border mb-5">
                {{!-- profile --}}
                <div class="tab-pane active" id="profile">
                    <div class="col-md-12">
                        <form action="/updatePi" method="POST" id="update">
                            <i style="font-size:24px" class="fa edit float-right mr-5">&#xf044;</i>

                            <div class="row">
                                <h6 class="mt-2 pl-3 pr-3">Personal Information
                                    <hr>
                                </h6>
                            </div>
                            <div class="row">
                                <div class="col-md-6 flex-nowrap">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label>Age:</label>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="text-nowrap"><input class="disabled" type="text" name="age"
                                                    value="{{result.Profile.Profile2.age}}" required></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label class="text-nowrap">Date of Birth:</label>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="text-nowrap"><input class="disabled" type="text" name="dob"
                                                    value="{{result.Profile.Profile2.dob.day}}/{{result.Profile.Profile2.dob.month}}/{{result.Profile.Profile2.dob.year}}"
                                                    required>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label class="text-nowrap">Maritial Status:</label>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="text-nowrap"><input class="disabled" type="text"
                                                    name="maritialstatus"
                                                    value="{{result.Profile.Profile2.maritialstatus}}" required></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label>Height:</label>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="text-nowrap"><input class="disabled" type="text" name="height"
                                                    value="{{result.Profile.Profile2.height}}" required></div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-4">
                                            <label class="text-nowrap">Posted by:</label>
                                        </div>
                                        <div class="col-md-8">
                                            <div class="text-nowrap"><input type="text" class="disabled"
                                                    value="{{result.Signup.createdBy}}" required></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 flex-nowrap">
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="text-nowrap">Mother Tongue:</label>
                                        </div>
                                        <div class="col-md-7">
                                            <div class="text-nowrap"><input class="disabled" type="text"
                                                    name="mothertongue" value="{{result.Profile.Profile2.mothertongue}}"
                                                    required></div>
                                        </div>
                                    </div>
                                    <div class="row ">
                                        <div class="col-md-5">
                                            <label>City:</label>
                                        </div>
                                        <div class="col-md-7">
                                            <div contenteditable="true" class="text-nowrap"><input class="disabled"
                                                    type="text" name="city"
                                                    value="{{result.Profile.Profile1.location.city}}" required></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="text-nowrap">Location:</label>
                                        </div>
                                        <div class="col-md-7 ">
                                            <div class="text-nowrap"><input class="disabled" type="text" name="country"
                                                    value="{{result.Profile.Profile1.location.country}}" required></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="text-nowrap">State:</label>
                                        </div>
                                        <div class="col-md-7 ">
                                            <div class="text-nowrap"><input class="disabled" type="text" name="state"
                                                    value="{{result.Profile.Profile1.location.state}}" required></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-5">
                                            <label class="text-nowrap">Body Weight:</label>
                                        </div>
                                        <div class="col-md-7">
                                            <div class="text-nowrap"><input class="disabled" type="text" name="weight"
                                                    value="{{result.Profile.Profile2.weight}}" required></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{!-- basic Information --}}
                            <div class="col-md-12">

                                <div class="row">
                                    <h6 class="mt-2"> Basics &amp; Lifestyle
                                        <hr>
                                    </h6>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 flex-nowrap">
                                        <div class="row ">
                                            <div class="col-md-4">
                                                <label>Diet:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <div class="text-nowrap"><input class="disabled" type="text" name="diet"
                                                        value="{{result.Profile.Profile2.diet}}" required></div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Sun Sign:</label>
                                            </div>
                                            <div class="col-md-7">
                                                {{#if result.Profile.Profile3.sunshine}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="sunshine" value="{{result.Profile.Profile3.sunshine}}">
                                                </div>
                                                {{else}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="sunshine" value="Update"></div>
                                                {{/if}}
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Personal Values:</label>
                                            </div>
                                            <div class="col-md-7 ">
                                                {{#if result.Profile.Profile3.personalvalues}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="personalvalues"
                                                        value="{{result.Profile.Profile3.personalvalues}}"></div>
                                                {{else}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="personalvalues" value="enter here"></div>
                                                {{/if}}
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Grew Up in:</label>
                                            </div>
                                            <div class="col-md-7 ">
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="grewup"
                                                        value="{{result.Profile.Profile1.location.country}}" required>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6 flex-nowrap">
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Blood Group:</label>
                                            </div>
                                            <div class="col-md-7">
                                                {{#if result.Profile.Profile3.bloodgroup}}
                                                <div class="text-nowrap"><input class="disabled" name="bloodgroup"
                                                        type="text" value="{{result.Profile.Profile3.bloodgroup}}">
                                                </div>
                                                {{else}}
                                                <div class="text-nowrap"><input class="disabled" name="bloodgroup"
                                                        type="text" value="Update"></div>
                                                {{/if}}
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Health Information:</label>
                                            </div>
                                            <div class="col-md-7">
                                                {{#if result.Profile.Profile3.healthinfo}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="healthinfo"
                                                        value="{{result.Profile.Profile3.healthinfo}}"></div>
                                                {{else}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="healthinfo" value="update"></div>
                                                {{/if}}

                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Disability:</label>
                                            </div>
                                            <div class="col-md-7">
                                                {{#if result.Profile.Profile3.disability}}
                                                <div><input class="disabled" type="text" name="disability"
                                                        value="{{result.Profile.Profile3.disability}}"> </div>
                                                {{else}}
                                                <div><input class="disabled" type="text" name="disability"
                                                        value="Not specified"> </div>
                                                {{/if}}
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Fluent in:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <div>
                                                    {{#if result.Profile.Profile3.fluent}}
                                                    <div><input class="disabled" type="text" name="fluent"
                                                            value="{{result.Profile.Profile3.fluent}}"></div>
                                                    {{else}}
                                                    <div><input class="disabled" type="text" name="fluent"
                                                            value="Update"></div>
                                                    {{/if}}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{!-- Religious Background --}}
                            <div class="col-md-12">

                                <div class=" row ">
                                    <h6 class="mt-2 "> Religious Background
                                        <hr>
                                    </h6>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 flex-nowrap">
                                        <div class="row ">
                                            <div class="col-md-4">
                                                <label>Religion:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="religion" value="{{result.Profile.Profile2.religion}}"
                                                        required>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Caste:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="caste" value="{{result.Profile.Profile2.caste}}" required>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Sub caste:</label>
                                            </div>
                                            <div class="col-md-8">
                                                {{#if result.Profile.Profile3.subcaste }}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="subcaste" value="{{result.Profile.Profile3.subcaste}}">
                                                </div>
                                                {{else}}
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="subcaste" value="Not Specified">
                                                </div>
                                                {{/if}}

                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Gothra:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="text-nowrap">
                                                    {{#if result.Profile.Profile3.gothra}}
                                                    <input class="disabled" type="text" name="gothra"
                                                        value="{{result.Profile.Profile3.gothra}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="gothra"
                                                        value="Not Specified">
                                                    {{/if}}
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-6 flex-nowrap">
                                        <div class="row ">
                                            <div class="col-md-5">
                                                <label>Rashi:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <div class="text-nowrap">
                                                    {{#if result.Profile.profile3.rashi}}
                                                    <input class="disabled" type="text" name="rashi"
                                                        value="{{result.Profile.profile3.rashi}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="rashi"
                                                        value="Not Specified">
                                                    {{/if}}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Manglik :</label>
                                            </div>
                                            <div class="col-md-7 ">
                                                <div class="text-nowrap">
                                                    {{#if result.Profile.Profile3.manglik}}
                                                    <input class="disabled" name="manglik" type="text"
                                                        value="{{result.Profile.Profile3.manglik}}">
                                                    {{else}}
                                                    <input class="disabled" name="manglik" type="text"
                                                        value="Will tell you later">
                                                    {{/if}}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">City of Birth:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <div class="text-nowrap">
                                                    {{#if result.Profile.Profile3.birthcity}}
                                                    <input class="disabled" type="text" name="birthcity"
                                                        value="{{result.Profile.Profile3.birthcity}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="birthcity"
                                                        value="Not spacified">
                                                    {{/if}}

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            {{!-- family-details  --}}
                            <div class="col-md-12">

                                <div class=" row ">
                                    <h6 class="mt-2 pl-3"> Family details
                                        <hr>
                                    </h6>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 flex-nowrap">
                                        <div class="row ">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Father's Status:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div>
                                                    {{#if result.Profile.Profile3.fatherstatus}}
                                                    <input class="disabled" type="text" name="fatherstatus"
                                                        value="{{result.Profile.Profile3.fatherstatus}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="fatherstatus"
                                                        value="Update">
                                                    {{/if}}

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Mother's Status:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div>
                                                    {{#if result.Profile.Profile3.motherstatus}}
                                                    <input class="disabled" type="text" name="motherstatus"
                                                        value="{{result.Profile.Profile3.motherstatus}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="motherstatus"
                                                        value="Update">
                                                    {{/if}}

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Family Location:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="text-nowrap">
                                                    {{#if result.Profile.Profile3.familylocation}}
                                                    <input class="disabled" type="text" name="familylocation"
                                                        value="{{result.Profile.Profile3.familylocation}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="familylocation"
                                                        value="Not specified">
                                                    {{/if}}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">Native Place:</label>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="text-nowrap">
                                                    {{#if result.Profile.Profile3.nativeplace}}
                                                    <input class="disabled" type="text" name="nativeplace"
                                                        value="{{result.Profile.Profile3.nativeplace}}">
                                                    {{else}}
                                                    <input class="disabled" type="text" name="nativeplace"
                                                        value="Not specified">
                                                    {{/if}}

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <label class="text-nowrap">No. of Siblings:</label>
                                            </div>
                                            <div class="col-md-8">
                                                {{#if result.Profile.Profile3.siblings}}
                                                <div><input class="disabled" type="text" name="siblings"
                                                        value="{{result.Profile.Profile3.siblings}}"></div>
                                                {{else}}
                                                <div><input class="disabled" type="text" name="siblings" value="Update">
                                                </div>
                                                {{/if}}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6 flex-nowrap">
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label>Family Status:</label>
                                            </div>
                                            <div class="col-md-7">
                                                <div class="text-nowrap"><input class="disabled" type="text"
                                                        name="status" value="{{result.Profile.Profile2.family.status}}"
                                                        required></div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Family Type:</label>
                                            </div>
                                            <div class="col-md-7 ">
                                                <div class="text-nowrap"><input class="disabled" type="text" name="type"
                                                        value="{{result.Profile.Profile2.family.type}}" required></div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-5">
                                                <label class="text-nowrap">Family Values:</label>
                                            </div>
                                            <div class="col-md-7">
                                                {{#if result.Profile.Profile3.familyvalues}}
                                                <div><input class="disabled" type="text" name="familyvalues"
                                                        value="{{result.Profile.Profile3.familyvalues}}">
                                                </div>
                                                {{else}}
                                                <div><input class="disabled" type="text" name="familyvalues"
                                                        value="Update">
                                                </div>
                                                {{/if}}
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            {{!-- Education-details  --}}
