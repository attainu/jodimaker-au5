    <section>
        <div class="container mt-5">
            <div class="row">
                {{!-- profile column --}}
                <div class="d-flex col-md-3">
                    <div class="card">
                        <div class="img-holder px-2 p-2">
                            <img src="https://s3.eu-central-1.amazonaws.com/bootstrapbaymisc/blog/24_days_bootstrap/robert.jpg"
                                class="card-img-top rounded-circle" alt="Profile Page">
                        </div>
                        <div class="card-body">
                            <hr>
                            <h5 class="card-title">FirstName LastName</h5>
                            <p class="card-text text-center">Profile completion: <mark>80
                                    %</mark>
                            </p>
                            <hr>
                            <div class="row ">
                                <a href="#" class="btn btn-primary mx-auto">Upload More Photos</a>
                            </div>

                        </div>
                    </div>
                </div>
                {{!-- requests row --}}
                <div class="col-md-6">
                    <h6>Your Activity</h6>
                    <div class=" d-flex pb-2 ">
                        <div class="col-md-4">
                            <a href="" class="btn btn-success active" data-toggle="tab"
                                data-target="#acceptedReqs">Accepted
                                Requests</a>
                        </div>
                        <div class="col-md-4">
                            <a href="" class="btn btn-primary" data-toggle="tab" data-target="#rejectedReqs">Recieved
                                Requests</a>
                        </div>
                        <div class="col-md-4">
                            <a href="" class="btn btn-secondary" data-toggle="collapse" href="#pendingReqs"
                                role="button" aria-expanded="false" aria-controls="pendingReqs">Pending Requests</a>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="tab-content">
                                <div class="tab-pane card border-success active" id="acceptedReqs">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col-md-2">
                                            <img src="https://s3.eu-central-1.amazonaws.com/bootstrapbaymisc/blog/24_days_bootstrap/robert.jpg"
                                                class="card-img-top rounded-circle p-3" alt="Profile Page">
                                        </div>
                                        <div class="col-md-8">
                                            <div class="card-body">
                                                <h6 class="card-title">FirstName lastname</h6>
                                                <p class="card-text">Age, height,mother tongue, location</p>
                                                <p class="card-text">
                                                    <small class="text-muted">Last online 3 mins ago</small>
                                                </p>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <a href="profile" class="btn btn-info">View</a>
                                        </div>
                                        <div class="col-md-2">
                                            <img src="https://s3.eu-central-1.amazonaws.com/bootstrapbaymisc/blog/24_days_bootstrap/robert.jpg"
                                                class="card-img-top rounded-circle p-3" alt="Profile Page">
                                        </div>
                                        <div class="col-md-8">
                                            <div class="card-body">
                                                <h6 class="card-title">FirstName lastname</h6>
                                                <p class="card-text">Age, height,mother tongue, location</p>
                                                <p class="card-text">
                                                    <small class="text-muted">Last online 3 mins ago</small>
                                                </p>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <a href="profile" class="btn btn-info">View</a>
                                        </div>
                                        <div class="col-md-2">
                                            <img src="https://s3.eu-central-1.amazonaws.com/bootstrapbaymisc/blog/24_days_bootstrap/robert.jpg"
                                                class="card-img-top rounded-circle p-3" alt="Profile Page">
                                        </div>
                                        <div class="col-md-8">
                                            <div class="card-body">
                                                <h6 class="card-title">FirstName lastname</h6>
                                                <p class="card-text">Age, height,mother tongue, location</p>
                                                <p class="card-text">
                                                    <small class="text-muted">Last online 3 mins ago</small>
                                                </p>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <a href="profile" class="btn btn-info">View</a>
                                        </div>
                                    </div>
                                </div>


                                {{!-- rejects --}}
                                <div class="tab-pane card border-primary" id="rejectedReqs">
                                    <div class="row no-gutters align-items-center">
                                        <div class="col-md-2">
                                            <img src="https://s3.eu-central-1.amazonaws.com/bootstrapbaymisc/blog/24_days_bootstrap/robert.jpg"
                                                class="card-img-top rounded-circle p-3" alt="Profile Page">
                                        </div>
                                        <div class="col-md-8">
                                            <div class="card-body">
                                                <h6 class="card-title">FirstName lastname</h6>
                                                <p class="card-text">Age, height,mother tongue, location</p>
                                                <p class="card-text">
                                                    <small class="text-muted">Last online 3 mins ago</small>
